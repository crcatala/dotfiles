// Place your key bindings in this file to overwrite the defaults
[
    {
        "command": "cursorLeft",
        "key": "cmd+h",
        "when": "editorTextFocus"
    },
    {
        "command": "cursorRight",
        "key": "cmd+i",
        "when": "editorTextFocus"
    },
    {
        "command": "cursorUp",
        "key": "cmd+e",
        "when": "editorTextFocus"
    },
    {
        "command": "cursorDown",
        "key": "cmd+n",
        "when": "editorTextFocus"
    },
    // Tab Navigation
    {
        "key": "cmd+1",
        "command": "workbench.action.previousEditor"
    },
    {
        "key": "cmd+2",
        "command": "workbench.action.nextEditor"
    },
    // Quick open navigation
    {
        "key": "cmd+n",
        "command": "workbench.action.quickOpenSelectNext",
        "when": "inQuickOpen"
    },
    {
        "key": "cmd+e",
        "command": "workbench.action.quickOpenSelectPrevious",
        "when": "inQuickOpen"
    },
    {
        "key": "cmd+n",
        "command": "keybindings.editor.focusKeybindings",
        "when": "inKeybindings && inKeybindingsSearch"
    },
    {
        "key": "cmd+n",
        "command": "repl.action.historyNext",
        "when": "editorTextFocus && inDebugRepl && onLastDebugReplLine"
    },
    {
        "key": "cmd+n",
        "command": "settings.action.focusSettingsFile",
        "when": "inSettingsSearch"
    },
    {
        "key": "cmd+n",
        "command": "search.focus.nextInputBox",
        "when": "inputBoxFocus && searchViewletVisible"
    },
    {
        "key": "cmd+n",
        "command": "workbench.action.interactivePlayground.arrowDown",
        "when": "interactivePlaygroundFocus && !editorTextFocus"
    },
    {
        "key": "cmd+n",
        "command": "showNextParameterHint",
        "when": "editorTextFocus && parameterHintsMultipleSignatures && parameterHintsVisible"
    },
    {
        "key": "cmd+n",
        "command": "selectNextSuggestion",
        "when": "editorTextFocus && suggestWidgetMultipleSuggestions && suggestWidgetVisible"
    },
    {
        "key": "cmd+n",
        "command": "list.focusDown",
        "when": "listFocus"
    },
    {
        "key": "cmd+e",
        "command": "repl.action.historyPrevious",
        "when": "editorTextFocus && inDebugRepl && onFirsteDebugReplLine"
    },
    {
        "key": "cmd+e",
        "command": "search.focus.previousInputBox",
        "when": "inputBoxFocus && searchViewletVisible"
    },
    {
        "key": "cmd+e",
        "command": "workbench.action.interactivePlayground.arrowUp",
        "when": "interactivePlaygroundFocus && !editorTextFocus"
    },
    {
        "key": "cmd+e",
        "command": "showPreviousParameterHint",
        "when": "editorTextFocus && parameterHintsMultipleSignatures && parameterHintsVisible"
    },
    {
        "key": "cmd+e",
        "command": "selectPrevSuggestion",
        "when": "editorTextFocus && suggestWidgetMultipleSuggestions && suggestWidgetVisible"
    },
    {
        "key": "cmd+e",
        "command": "list.focusUp",
        "when": "listFocus"
    },
    // File List Navigation
    {
        "key": "cmd+h",
        "command": "list.collapse",
        "when": "listFocus"
    },
    {
        "key": "cmd+i",
        "command": "list.expand",
        "when": "listFocus"
    },
    {
        "command": "cursorPageUp",
        "key": "cmd+u",
        "when": "editorTextFocus"
    },
    {
        "command": "cursorPageDown",
        "key": "cmd+l",
        "when": "editorTextFocus"
    },
    {
        "command": "cursorLeftSelect",
        "key": "shift+cmd+h",
        "when": "editorTextFocus"
    },
    {
        "command": "cursorRightSelect",
        "key": "shift+cmd+i",
        "when": "editorTextFocus"
    },
    {
        "command": "cursorUpSelect",
        "key": "shift+cmd+e",
        "when": "editorTextFocus"
    },
    {
        "command": "cursorDownSelect",
        "key": "shift+cmd+n",
        "when": "editorTextFocus"
    },
    {
        "command": "cursorPageDownSelect",
        "key": "shift+cmd+l",
        "when": "editorTextFocus"
    },
    {
        "command": "cursorWordEndRight",
        "key": "cmd+y",
        "when": "editorTextFocus"
    },
    {
        "command": "cursorWordStartLeft",
        "key": "cmd+j",
        "when": "editorTextFocus"
    },
    {
        "command": "editor.action.addSelectionToNextFindMatch",
        "key": "cmd+t",
        "when": "editorFocus"
    },
    {
        "command": "editor.action.copyLinesDownAction",
        "key": "cmd+d",
        "when": "editorTextFocus"
    },
    {
        "command": "editor.action.moveLinesUpAction",
        "key": "shift+cmd+u",
        "when": "editorTextFocus"
    },
    {
        "command": "editor.action.moveLinesDownAction",
        "key": "shift+cmd+l",
        "when": "editorTextFocus"
    },
    {
        "command": "editor.action.deleteLines",
        "key": "cmd+shift+d",
        "when": "editorTextFocus"
    },
    {
        "command": "workbench.files.action.focusFilesExplorer",
        "key": "cmd+k cmd+f"
    },
    {
        "key": "cmd+k cmd+i",
        "command": "workbench.action.terminal.focus",
        "when": "editorFocus"
    },
    {
        "command": "workbench.action.focusNextGroup",
        "key": "cmd+shift+]"
    },
    {
        "command": "workbench.action.focusPreviousGroup",
        "key": "cmd+shift+["
    },
    {
        "key": "cmd+k cmd+t",
        "command": "workbench.action.terminal.focus"
    },
    {
        "key": "cmd+k cmd+e",
        "command": "workbench.action.focusActiveEditorGroup"
    },
    {
        "key": "cmd+k cmd+[",
        "command": "workbench.action.splitEditor"
    },
    {
        "key": "cmd+k cmd+]",
        "command": "workbench.action.splitEditor"
    }
]